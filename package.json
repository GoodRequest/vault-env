    "@types/config": "3.3.0",
    "@types/lodash": "4.14.191",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "commitlint-config-jira": "1.6.4",
    "commitlint-plugin-jira-rules": "1.6.4",
    "eslint": "8.33.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "prettier": "2.8.3",
    "shx": "0.3.4",
    "typescript": "4.9.5"
